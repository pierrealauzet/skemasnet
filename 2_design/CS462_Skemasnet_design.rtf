\par \pard\plain \ltrpar\s7\cf0\sl276\slmult1\qj{\*\hyphen2\hyphlead2\hyphtrail2\hyphmax0}\aspalpha\rtlch\af6\afs22\lang1025\ltrch\dbch\af6\langfe1042\hich\f6\fs22\lang1033\loch\f6\fs22\lang1033 
